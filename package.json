{
  "name": "blog-api",
  "version": "1.0.0",
  "description": "API Node.js pour authentification et gestion d'articles de blog",
  "main": "dist/start.js",
  "scripts": {
    "start": "node dist/start.js",
    "dev": "nodemon --exec ts-node src/start.ts",
    "build": "tsc",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "format": "prettier --write .",
    "format:check": "prettier --check ."
  },
  "keywords": [
    "nodejs",
    "express",
    "mongodb",
    "jwt",
    "blog",
    "api"
  ],
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-rate-limit": "^6.10.0",
    "express-validator": "^7.0.1",
    "helmet": "^7.0.0",
    "jsonwebtoken": "^9.0.2",
    "mongoose": "^7.5.0",
    "morgan": "^1.10.0",
    "swagger-ui-express": "^5.0.1"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.6",
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.3",
    "@types/helmet": "^0.0.48",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/mongoose": "^5.11.96",
    "@types/morgan": "^1.9.10",
    "@types/node": "^24.1.0",
    "@types/swagger-ui-express": "^4.1.8",
    "eslint": "^8.45.0",
    "nodemon": "^3.1.10",
    "prettier": "^3.0.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.8.3"
  }
}
